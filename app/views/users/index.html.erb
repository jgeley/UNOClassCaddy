<!-- Put the table of selected classes into a div for styling -->
<div id="selectedclasses">
<table>
	<thead>
		<tr>
			<th>Department</th>
			<th>Course</th>
			<th></th>
		</tr>
	</thead>

	<tbody>
		<% UnoClass.all.each do |uno_class| %>
		<%if uno_class.sessionId.to_s == request.session_options[:id].to_s%>
		<tr>
			<td><%= uno_class.department %></td>
			<td><%= uno_class.course %></td>
			<td><%= link_to 'Remove', uno_class, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		</tr>
		<%end%>
		<% end %>
	</tbody>
</table>
</div>
<div id="message" data-lat="<%= session[:message] %>"><%= session[:message]%></div>
