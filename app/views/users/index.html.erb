<script>
var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
var message = $("#message").data("lat");
$(document).ready(function(){
	if(message != ""){
    	alert(message);
    }
    if(!is_chrome){
    	alert("This website only works in chrome");
    }
  });
</script>
</html>


